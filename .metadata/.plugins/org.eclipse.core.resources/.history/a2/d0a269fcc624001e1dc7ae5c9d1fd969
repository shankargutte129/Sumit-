package Basic;

public class PrimitiveDatatypes {

	public PrimitiveDatatypes() {
	}

	public static void main(String[] args) {
		//byte  1 byte -128 to +127
		//short 2 byte  1,2,3
		//int   4 byte  1,2,3
		//long  8 byte  1,2,3
		//float 4 byte  3.146789
		//double 8 byte 6.4567890
		//char   2 byte a,B,A,c
		//boolean 1 byte True,False
		
		byte   a=127;
		short  b=1234;
		int    c=1234567890;
		long   d=7276713070L;
		float  e=3.14f;
		double f=123.4567D;
		char   g='S';
		boolean h=true;
	System.out.println(a+"/n"+b+"/n"+c+"/n"+d+"/n"+e+"/n"+f+"/n"+g+"/n"+h);
		
		
		
		
		

	}

}
