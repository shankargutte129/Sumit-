package Shankar;
import java.util.Scanner;

public class termsofAP {
	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
	int x=s.nextInt();
	
	int n=1;
	for (int i=0; i<x; i++) {
	  int term =3*i+2;
		 if (term%4!=0) 
		 {
			 System.out.print(term+" "); 
		 }
		 n++;
	} 
}
}